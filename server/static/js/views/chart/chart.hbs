<div class="card">
    <div class="card-header">
        <i class="{{ card-icon }} fa-lg" aria-hidden="true"></i>
        <p class="card-header-title" title="{{ title }}">{{ title }}</p>
    </div>
    <div class="card-body-container">
        <div class="card-body">
            <canvas class="card-chart" height="400px" width="512px"></canvas>
        </div>
    </div>
</div>